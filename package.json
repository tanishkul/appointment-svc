{
  "name": "appointment-svc",
  "version": "1.0.0",
  "description": "Appointment Booking Service",
  "main": "index.js",
  "scripts": {
    "prebuild": "npm run remove-dist",
    "postbuild": "npm run copy-resources",
    "build": "tsc",
    "prestart": "npm run remove-dist",
    "start": "tsc-watch --project ./ --onSuccess \"npm run nodemon\"",
    "startup": "npm run build && node ./dist/src/index.js",
    "prenodemon": "npm run copy-resources",
    "nodemon": "nodemon ./dist/src/index.js",
    "packaging": "npm run build && pkg ./dist/",
    "remove-dist": "rm -rf ./dist",
    "copy-resources": "cp -Rf ./package.json ./dist",
    "pretest": "npm run build",
    "test": "cross-env NODE_ENV=test jest --forceExit --runInBand --detectOpenHandles",
    "watch-test": "yarn run test -- --watchAll",
    "watch-test:ci": "yarn run watch-test -- --coverage",
    "test:c": "yarn run test -- --coverage",
    "precommit": "CI=true yarn lint && yarn test",
    "commit": "git-cz",
    "push": "git push",
    "lint": "./node_modules/tslint/bin/tslint -c tslint.json -p tsconfig.json --exclude 'node_modules/**/*.{j,t}s'",
    "lint:fix": "yarn lint -- --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tanishkul/appointment-svc.git"
  },
  "author": "Tanish Kulshrestha",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/tanishkul/appointment-svc/issues"
  },
  "homepage": "https://github.com/tanishkul/appointment-svc#readme",
  "dependencies": {
    "@types/ejs": "2.6.3",
    "@types/express-jwt": "0.0.42",
    "@types/moment-timezone": "0.5.12",
    "@types/node": "13.9.8",
    "@types/nodemailer": "6.2.0",
    "axios": "^0.21.1",
    "body-parser": "1.18.3",
    "compression": "1.7.2",
    "cookie-parser": "1.4.3",
    "cors": "2.8.4",
    "cron": "1.8.2",
    "dotenv": "6.0.0",
    "ejs": "2.6.2",
    "express": "4.16.3",
    "express-jwt": "5.3.1",
    "express-validator": "5.3.0",
    "fs": "0.0.1-security",
    "helmet": "3.23.3",
    "http": "0.0.0",
    "https": "1.0.0",
    "method-override": "2.3.10",
    "moment-timezone": "0.5.26",
    "mongoose": "^5.12.14",
    "morgan-body": "2.4.5",
    "nodemailer": "^6.6.1",
    "nodemailer-smtp-transport": "2.7.4",
    "objects-to-csv": "1.3.6",
    "path": "0.12.7",
    "perf_hooks": "0.0.1",
    "query-string": "6.8.1",
    "retry": "0.12.0",
    "sinon": "7.3.2",
    "swagger-jsdoc": "1.9.7",
    "swagger-ui-express": "3.0.10",
    "yarn": "1.12.3"
  },
  "devDependencies": {
    "@types/bluebird": "3.5.21",
    "@types/compression": "0.0.36",
    "@types/cookie-parser": "1.4.1",
    "@types/cors": "2.8.4",
    "@types/cron": "1.7.2",
    "@types/dotenv": "4.0.3",
    "@types/express": "4.16.0",
    "@types/express-jwt": "0.0.42",
    "@types/faker": "4.1.2",
    "@types/helmet": "0.0.38",
    "@types/jest": "23.3.1",
    "@types/method-override": "0.0.31",
    "@types/mongoose": "5.2.0",
    "@types/qs": "6.9.0",
    "@types/supertest": "2.0.4",
    "@types/swagger-jsdoc": "0.0.2",
    "@types/swagger-ui-express": "3.0.0",
    "@types/winston": "2.3.9",
    "commitizen": "2.10.1",
    "cross-env": "5.2.0",
    "cz-conventional-changelog": "2.1.0",
    "faker": "4.1.0",
    "handlebars": "^4.7.7",
    "husky": "0.14.3",
    "jest": "23.5.0",
    "jest-sonar-reporter": "2.0.0",
    "mongodb-memory-server": "3.1.2",
    "nodemon": "1.18.7",
    "sonarjs": "1.0.0",
    "supertest": "3.1.0",
    "ts-jest": "23.0.0",
    "tsc-watch": "1.0.26",
    "tslint": "5.10.0",
    "typedoc": "0.17.8",
    "typescript": "3.8.3"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "pkg": {
    "assets": [
      "dist/**/*"
    ],
    "targets": [
      "node10"
    ]
  }
}
